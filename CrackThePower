import gmpy2

e = 20
c = 

root, exact = gmpy2.iroot(c, e)

if not exact:
    print("Not found (root is not an exact e-th root)")
else:
    # print the integer root
    print("root (int):", root)

    # convert to bytes
    b = int(root).to_bytes((root.bit_length() + 7) // 8, 'big')
    # print raw bytes length and first bytes for sanity
    print("bytes length:", len(b))
    print("first 64 bytes (hex):", b[:64].hex())

    # try to decode as UTF-8 (safe)
    try:
        s = b.decode('utf-8')
        print("decoded string:\n", s)
    except UnicodeDecodeError:
        print("decoded string: (not valid UTF-8)")
